{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "rustfeed",
  "version": "0.9.0",
  "identifier": "com.rustfeed.gui",
  "build": {
    "beforeBuildCommand": {
      "script": "npm run build",
      "cwd": "../../apps/rustfeed-gui"
    },
    "beforeDevCommand": {
      "script": "npm run dev",
      "cwd": "../../apps/rustfeed-gui"
    },
    "devUrl": "http://localhost:5173",
    "frontendDist": "../../apps/rustfeed-gui/dist"
  },
  "app": {
    "windows": [
      {
        "title": "rustfeed",
        "width": 1200,
        "height": 800,
        "resizable": true,
        "fullscreen": false,
        "minWidth": 800,
        "minHeight": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.ico",
      "icons/icon.png"
    ]
  }
}
